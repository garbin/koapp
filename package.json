{
  "name": "Koapp",
  "title": "Koapp 应用",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "node --harmony index.js",
    "test": "export NODE_ENV=test && npm start lint && npm start database reset -- -f && jest --forceExit"
  },
  "koapi": {
    "commands": {
      "path": "./app/commands",
      "default": [
        "universal"
      ]
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/koapi/koapp.git"
  },
  "browser": {
    "joi": "joi-browser"
  },
  "standard": {
    "parser": "babel-eslint",
    "ignore": [
      "/docs",
      "/storage"
    ]
  },
  "babel": {
    "presets": [
      "latest",
      "react",
      "stage-0"
    ],
    "plugins": [
      "react-hot-loader/babel"
    ]
  },
  "jest": {
    "transform": {
      ".*": "<rootDir>/node_modules/babel-jest"
    },
    "testEnvironment": "node",
    "testRegex": "(/tests/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "js",
      "json",
      "es"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/lib",
      "__lib__"
    ],
    "setupFiles": [
      "<rootDir>/node_modules/babel-polyfill/lib"
    ]
  },
