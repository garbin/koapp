  },
  "browser": {
    "joi": "joi-browser"
  },
  "standard": {
    "parser": "babel-eslint",
    "ignore": [
      "/docs",
      "/storage"
    ]
  },
  "babel": {
    "presets": [
      "latest",
      "react",
      "stage-0"
    ],
    "plugins": [
      "react-hot-loader/babel",
      "styled-jsx/babel",
      [
        "transform-runtime",
        {
          "helpers": false,
          "polyfill": false,
          "regenerator": true,
          "moduleName": "babel-runtime"
        }
      ]
    ]
  },
  "jest": {
    "transform": {
      "^.+\\.jsx$": "babel-jest",
      "/app/clients/.+\\.jsx?$": "babel-jest"
    },
    "testEnvironment": "node",
    "testRegex": "(/tests/.*|\\.(test|spec))\\.(ts|tsx|js|jsx)$",
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json",
      "es"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/lib",
      "__lib__"
    ],
    "setupFiles": [
      "<rootDir>/node_modules/babel-polyfill/lib"
    ]
  },
