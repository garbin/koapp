{
  "name": "Koapp",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "export NODE_ENV=production && npm run build && node server.js",
    "test": "NODE_ENV=test make test",
    "test:coverage": "NODE_ENV=test make test-cover",
    "dev": "concurrently \"npm run dev:client -- --port 5001\" \"npm run dev:server\" \"json-server db.json --port 5002\"",
    "dev:client": "NODE_ENV=development webpack-dev-server -d --history-api-fallback --hot --inline --progress --colors --host 0.0.0.0",
    "dev:server": "NODE_ENV=development nodemon --watch src/server.jsx -L -e js,es,jsx server.js",
    "build": "concurrently \"npm run build:client\" \"npm run build:server\"",
    "build:client": "rm -rf ./static && webpack --progress --colors",
    "build:server": "rm -rf ./build && babel --copy-files --presets es2015,stage-0,react -d build/ src/",
    "update": "export NODE_ENV=production && webpack --progress --colors",
    "error.log": "tail -f ./data/logs/error.log | bunyan",
    "serve:coverage": "make coverage-server"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/koapi/koapiend.git"
  },
  "browser": {
    "joi": "joi-browser"
  },
  "babel": {
    "presets": [
      "es2015",
      "react",
      "stage-0"
    ],
    "plugins": [
      "react-hot-loader/babel"
    ]
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/koapi/koapiend/issues"
  },
  "homepage": "https://github.com/koapi/koapiend#readme",
  "dependencies": {
    "axios": "^0.13.1",
    "bootstrap": "^3.3.6",
    "concurrently": "^2.2.0",
    "joi": "^9.0.1",
    "joi-browser": "^9.0.0",
    "joi-validation-strategy": "^0.3.3",
    "jquery": "^2.2.2",
    "koa-mount": "^1.3.0",
    "koa-proxy": "^0.6.0",
    "koa-static-server": "^0.1.8",
    "koapi": "^0.2.21",
    "lodash": "^4.6.1",
    "moment": "^2.14.1",
    "node-sass": "^3.6.0",
    "react": "^15.2.1",
    "react-dom": "^15.2.1",
    "react-helmet": "^3.1.0",
    "react-loader": "^2.4.0",
    "react-redux": "^4.4.5",
    "react-router": "^2.0.1",
    "react-router-redux": "^4.0.5",
    "react-validation-mixin": "^5.3.4",
    "redux": "^3.3.1",
    "redux-actions": "^0.10.1",
    "redux-async-connect": "^1.0.0-rc4",
    "redux-effects": "^0.4.3",
    "redux-effects-credentials": "^0.1.12",
    "redux-form": "^6.0.0-rc.3",
    "redux-promise": "^0.5.3",
    "redux-promise-middleware": "^3.3.2",
    "redux-thunk": "^2.0.1",
    "serialize-javascript": "^1.3.0",
    "style-loader": "^0.13.1",
    "webpack-isomorphic-tools": "^2.3.2"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.4",
    "babel": "^6.5.2",
    "babel-cli": "^6.10.1",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "css-loader": "^0.23.1",
    "expect": "^1.16.0",
    "expose-loader": "^0.7.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "http-server": "^0.9.0",
    "istanbul": "^1.0.0-alpha",
    "json-server": "^0.8.16",
    "mocha": "^2.4.5",
    "nodemon": "^1.9.2",
    "postcss-loader": "^0.8.2",
    "react-addons-test-utils": "^15.0.1",
    "react-hot-loader": "^3.0.0-beta.2",
    "redux-mock-store": "^1.0.2",
    "rucksack-css": "^0.8.5",
    "sass-loader": "^4.0.0",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.15",
    "webpack-dev-server": "^1.14.1"
  }
}
