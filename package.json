{
  "name": "Koapp",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "start": "node --harmony `which koapi`",
    "lint": "standard ./**/*.js ./**/*.es ./**/*.jsx",
    "test": "export NODE_ENV=test && npm run lint && npm start migrate reset && jest --forceExit"
  },
  "koapi": {
    "commands": {
      "path": "./app/server/commands",
      "default": [
        "universal"
      ]
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/koapi/koapiend.git"
  },
  "browser": {
    "joi": "joi-browser"
  },
  "standard": {
    "parser": "babel-eslint",
    "ignore": [
      "/docs",
      "/storage"
    ]
  },
  "babel": {
    "presets": [
      "latest",
      "react",
      "stage-0"
    ],
    "plugins": [
      "react-hot-loader/babel"
    ]
  },
  "jest": {
    "transform": {
      ".*": "<rootDir>/node_modules/babel-jest"
    },
    "testRegex": "(/tests/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "js",
      "json",
      "es"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/lib",
      "__lib__"
    ],
    "setupFiles": [
      "<rootDir>/node_modules/babel-polyfill/lib"
    ]
  },
  "author": "Garbin Huang",
  "license": "ISC",
  "bugs": {},
